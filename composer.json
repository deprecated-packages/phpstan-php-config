{
    "name": "symplify/phpstan-php-config",
    "description": "Use PHP config syntax to configure PHPStan in phpstan.php",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "phpstan/phpstan": "^0.12.80",
        "symfony/console": "^4.4|^5.2",
        "symplify/smart-file-system": "^9.2.8",
        "symplify/package-builder": "^9.2.8",
        "symplify/symplify-kernel": "^9.2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PHPStanPHPConfig\\": "src"
        }
    },
    "bin": [
        "bin/phpstan-php-config"
    ],
    "autoload-dev": {
        "psr-4": {
            "Symplify\\PHPStanPHPConfig\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
