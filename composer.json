{
    "name": "symplify/phpstan-php-config",
    "description": "Use PHP config syntax to configure PHPStan in phpstan.php",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "phpstan/phpstan": "^0.12.57",
        "symfony/console": "^4.4|^5.1",
        "symplify/smart-file-system": "^9.0.2",
        "symplify/package-builder": "^9.0.2",
        "symplify/symplify-kernel": "^9.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PHPStanPHPConfig\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\PHPStanPHPConfig\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
